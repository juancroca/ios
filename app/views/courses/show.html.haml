.container
  %p#notice= notice

  - if @course.closed
    .row
      %h2
        Result of distribution
        - if supervisor_signed_in?
          - @jobs.each do |job|
            %h4
              Job #
              = job.id
            - job.groups.each do |group|
              %h5= group.name
              .row
                %ul
                  - group.students.each do |student|
                    %li= student.name
        - if student_signed_in?
          %h5= @group.name
          .row
            %ul
              - @group.students.each do |student|
                %li= student.name
